programa {
  funcao inicio() {

    real numero1, numero2, resultado

    escreva("Digite um numero: ")
    leia(numero1)
    escreva("Digite um numero: ")
    leia(numero2)
    resultado = numero1/numero2
    escreva("O resultado da divisão desses numeros é: ", resultado)
    
    real n1, n2, soma
    escreva("\nDigite um número: ")
    leia(n1)
    escreva("Digite outro número: ")
    leia(n2) 
    soma = n1+n2
    escreva(soma)
  }
}